{
  "name": "ether-loto",
  "version": "0.0.1",
  "description": "Ethereum based lotery DApp",
  "main": "web/app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "gulp",
    "start-truffle": "concurrently \"cd ether-loto-contract && truffle dev\" \"cd ether-loto-contract && truffle migrate --reset && cd .. && gulp serve-full\"",
    "start-geth": "concurrently \"geth --dev --rpc --targetgaslimit 9000000000\" \"cd ether-loto-contract && truffle migrate --reset --network devGeth && cd .. &&  gulp serve-full\"",
    "start-ganache": "cd ether-loto-contract && truffle migrate --reset --network devGanache && cd .. && gulp serve-full",
    "deploy": "gulp deploy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "bower install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ethereum-game-company/ether-loto.git"
  },
